{
  "name": "Bilingual Aphasia Test Scorer",
  "version": "1.3.0",
  "description": "An WebApp to enter Bilingual Aphasia Test data, and to get a patient's profile. ",
  "app": {
    "launch": {
      "local_path": "view/main.html"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage"
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "manifest_version" : 2,
  "content_security_policy" : "default-src 'self';"
}
